#!/usr/bin/env bash

source ./scripts/circle/_init

set -x

echo "Releasing ${CHANNEL}"

ls tmp/darwin_x64
ls tmp/linux_x64
echo "SKIP RELEASES FOR NOW"
exit 1

# release to s3
"${root}/scripts/release_tarballs" dist/tarballs

if [[ "${OS}" = "darwin" ]]; then
  "${root}/scripts/darwin/homebrew"
fi

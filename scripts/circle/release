#!/usr/bin/env bash

source ./scripts/main
source ./scripts/circle/vars
./scripts/circle/github_setup

set -x

echo "Releasing ${CHANNEL}"


ls tmp/builds
ls tmp/builds/darwin_x64
ls tmp/builds/linux_x64
echo "SKIP RELEASES FOR NOW"
exit 1

# release to s3
"${root}/scripts/release_tarballs" dist/tarballs

if [[ "${OS}" = "darwin" ]]; then
  "${root}/scripts/darwin/homebrew"
fi
